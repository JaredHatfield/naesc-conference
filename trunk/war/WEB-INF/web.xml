<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
	</system-properties>

    <servlet>
        <servlet-name>editattendee</servlet-name>
        <servlet-class>com.naesc2011.conference.server.naesc.EditAttendeeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editaward</servlet-name>
        <servlet-class>com.naesc2011.conference.server.naesc.EditAwardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editcouncil</servlet-name>
        <servlet-class>com.naesc2011.conference.server.naesc.EditCouncilServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>com.naesc2011.conference.server.naesc.HomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mycouncil</servlet-name>
        <servlet-class>com.naesc2011.conference.server.naesc.MyCouncilServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com.naesc2011.conference.server.naesc.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>processregister</servlet-name>
        <servlet-class>com.naesc2011.conference.server.naesc.ProcessRegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>editattendee</servlet-name>
        <url-pattern>/editattendee</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editaward</servlet-name>
        <url-pattern>/editaward</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editcouncil</servlet-name>
        <url-pattern>/editcouncil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mycouncil</servlet-name>
        <url-pattern>/mycouncil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>processregister</servlet-name>
        <url-pattern>/process/register</url-pattern>
    </servlet-mapping>
    
